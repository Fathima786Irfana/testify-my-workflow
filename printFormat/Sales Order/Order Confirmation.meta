am = frappe.get_all('Sales Team', filters={'parent': doc.name}, fields=['sales_person']) -%}\\n{%- set sales_person_name = sales_team[0].sales_person if sales_team else None -%}\\n\\n{%- if sales_person_name -%}\\n    {%- set sales_person = frappe.get_doc('Sales Person', sales_person_name) -%}\\n    {%- set custom_contact = sales_person.custom_contact if sales_person else None -%}\\n    \\n    {%- if custom_contact -%}\\n        {%- set contact_name = custom_contact -%}\\n\\n        {%- set phone_details = frappe.get_all('Contact Phone', filters={'parent': contact_name, 'is_primary_phone': 1}, fields=['phone']) -%}\\n        {%- set email_details = frappe.get_all('Contact Email', filters={'parent': contact_name, 'is_primary': 1}, fields=['email_id']) -%}\\n\\n        {%- set contact_phone = phone_details[0].phone if phone_details else None -%}\\n        {%- set contact_email = email_details[0].email_id if email_details else None -%}\\n    {%- else -%}\\n        {%- set contact_phone = None -%}\\n        {%- set contact_email = None -%}\\n    {%- endif -%}\\n{%- else -%}\\n    {%- set contact_phone = None -%}\\n    {%- set contact_email = None -%}\\n{%- endif -%}\\n\\n{%- set contact_list = frappe.get_list('Contact', filters={'name': doc.agent}, fields=['phone', 'email_id']) -%}\\n{%- set contact = contact_list[0] if contact_list else {} -%}\\n<div class=\\\"col-xs-6\\\">\\n    <label style=\\\"color: black;\\\">{{ _(\\\"Your contact person for technical issues\\\") }}:</label>\\n    <div class=\\\"data-field\\\" data-fieldname=\\\"left_contact\\\" data-fieldtype=\\\"Small Text\\\">\\n        <div class=\\\"value\\\">\\n            <p>{{ sales_person_name }}</p>\\n            <p>{{ contact_email }}</p>\\n            <p>{{ contact_phone }}</p>\\n        </div>\\n    </div>\\n</div>\\n    \\n    <!-- Right Section -->\\n    <!--{%- set user_doc = frappe.get_doc('User', doc.owner) -%}-->\\n    <!--{%- set user_full_name = user_doc.full_name if user_doc else None -%}-->\