{
  "name": "offer-prism",
  "version": "1.0.0",
  "description": "LENS Auto Offer",
  "main": "getClientScript.js",
  "type": "module",
  "scripts": {
    "start": "cd files && node putClientScript.js && node putServerScript.js && node putReport.js",
    "get": "cd files && node getClientScript.js && node getServerScript.js && node getReport.js",
    "upload": "cd files && node uploadClientScript.js && node uploadServerScript.js && node uploadReport.js",
    "test": "npx cypress run --spec=cypress/e2e/*.cy.js --config-file=cypress.config.cjs",
    "get_field": "cd files && node getCustomField.js",
    "update_field": "cd files && node updateCustomField.js",
    "get_letterhead_printformat": "cd files && node getLetterAndPrintFormat.js",
    "upload_letterhead_printformat": "cd files && node uploadLetterAndPrintFormat.js",
    "get_perm": "cd files && node getRolePerm.js",
    "upload_perm": "cd files && node updateRolePerm.js",
    "get_doctype": "cd files && node getCustomDoctype.js",
    "update_doctype":"cd files && node updateCustomDoctype.js",
    "get_document": "cd files && node getDocument.js",
    "update_document": "cd files && node updateDocument.js",
    "get_user_permission": "cd files && node getUserPermission.js",
    "update_user_permission": "cd files && node uploadUserPermission.js",
    "delete_record": "cd files && node deleteScript.js"
  },
